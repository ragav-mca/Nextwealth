package practice_june;

public class NaturalNum {
	void SumNaturalNumbers()
	{
	int sum = 0;
	
	for(int i=1; i<=5; i++)
	{
		sum = sum + i;
	}
	System.out.println("The sum of first five natural numbers is "+sum);
	}

}

package practice_june;

public class NaturalNum {
	void SumNaturalNumbers()
	{
	int sum = 0;
	
	for(int i=1; i<=5; i++)
	{
		sum = sum + i;
	}
	System.out.println("The sum of first five natural numbers is "+sum);
	}

}

Output:
The sum of first five natural numbers is 15
