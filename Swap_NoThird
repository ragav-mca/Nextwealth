package sayur_practice;

public class Swap_NoThird {

	public static void main(String[] args) {
		int first = 20;
		int second = 50;
		System.out.println("Before swapping: "+"first :"+first+" second: "+second);
		
		first = first - second;
		second = first + second;
		first = second - first;
		System.out.println("After swapping: "+"first :"+first+" second: "+second);

	}

}

Output:
Before swapping: first :20 second: 50
After swapping: first :50 second: 20
