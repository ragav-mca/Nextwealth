package practice_june;

public class Factor1 {
		static void printFactors(int num)
		{
			for(int i=1; i<=num; i++)
			{
				if(num%i == 0)
				{
					System.out.print(i+" ");
				}
			}
	}

}

package practice_june;

import java.util.Scanner;

public class Factor1App {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		System.out.println("Enter the number to find the factors of it");
		int num = scan.nextInt();
		Factor1.printFactors(num);

	}

}

Output:
Enter the number to find the factors of it
8
1 2 4 8 

